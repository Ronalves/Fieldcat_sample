    gs_alv_fieldcat-fieldname = 'VBELN'.
    gs_alv_fieldcat-tabname   = '<T_TABLE>'.
    gs_alv_fieldcat-outputlen = '4'.
*    gs_alv_fieldcat-coltext   = 'Empresa'.
